alias a="php artisan"
alias t="php artisan tinker"
alias migrate="php artisan migrate"
alias rollback="php artisan migrate:rollback --step=1"
alias seed="php artisan db:seed"
alias fresh="php artisan migrate:fresh"
alias testing="php artisan migrate:fresh --seed --env=testing"
alias ac="php artisan cache:clear"
alias oc="php artisan optimize:clear"
alias key="php artisan key:generate"

alias p="./vendor/bin/phpunit"
alias pf="./vendor/bin/phpunit --filter"
alias pc="php artisan test --coverage"

alias c="composer"
alias cfix="composer app:cs-fix"
alias cs="composer app:cs"
alias cc="composer check"
alias cf="composer check-full"
alias ct="composer test"
